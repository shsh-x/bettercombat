        "TargetField": [ "Ghost", "9" ],
        "Value": "{{Round: {{Query: {{Data/Monsters:Ghost:9}} * {{FlyingDetectionMultiplier}} }} }}",
        "When": {
          "PatchValue": "Ghost"
        }
      },
      "When": {
        "FlyingMonsterDetection|not=Default": true
      }
    },
    // #endregion

