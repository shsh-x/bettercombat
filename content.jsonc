    {
      "LogName": "Monster Loot Drop Rate Adjustment",
      "Action": "ForEach",
      "Values": "{{MonsterList}}",
      "Patch": {
        "LogName": "Updating loot for {{PatchValue}}",
        "Action": "EditData",
        "Target": "Data/Monsters",
        "TargetField": [ "{{PatchValue}}", "6" ],
        "Value": "{{NewLootString_{{PatchValue | replace: ' ', '_'}}}}",
        "When": {
          "Query: '{{Data/Monsters:{{PatchValue}}:12}}' = 'true'": true
        }
      },
      "When": {
        "LootDropRate|not=Default": true
      }
    }
